          <TableColumn fx:id="colProvince" prefWidth="54.0" text="所在省"/>
          <TableColumn fx:id="colCountry" prefWidth="65.5" text="所在市"/>
          <TableColumn fx:id="colAddress" prefWidth="234.0" text="具体地址"/>
          <TableColumn fx:id="colAQI" prefWidth="79.0" text="预估AQI等级"/>
          <TableColumn fx:id="colTime" prefWidth="159.5" text="时间"/>
      </columns>
    </TableView>
    <Label layoutX="14.0" layoutY="14.0" text="反馈数据查询">
        <font>
          <Font name="System Bold" size="24.0"/>
        </font>
    </Label>
    <Button fx:id="btn_back" layoutX="273.0" layoutY="375.0" mnemonicParsing="false" onAction="#backToSubmit"
            text="返回"/>
</AnchorPane>
